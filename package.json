{
	"name": "@lesjoursfr/audio-waveform",
	"version": "3.1.7",
	"description": "A library to generate waveform for audio files.",
	"license": "MIT",
	"repository": "lesjoursfr/audio-waveform",
	"homepage": "https://github.com/lesjoursfr/audio-waveform#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/audio-waveform/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "20.x || 22.x"
	},
	"keywords": [
		"waveform",
		"generator",
		"audio"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build": "tsc && cp -R src/audio-analyzer lib",
		"test": "mocha"
	},
	"dependencies": {
		"electron": "^33.0.2",
		"mime-types": "^2.1.35",
		"uuid": "^10.0.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@tsconfig/node20": "^20.1.4",
		"@types/mime-types": "^2.1.4",
		"@types/mocha": "^10.0.9",
		"@types/node": "^20.17.2",
		"@types/yargs": "^17.0.33",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.11.0",
		"mocha": "^10.7.3",
		"prettier": "^3.3.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.1"
	},
	"packageManager": "yarn@4.5.1"
}
