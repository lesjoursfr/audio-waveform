{
	"name": "@lesjoursfr/audio-waveform",
	"version": "1.0.2",
	"description": "A library to generate waveform for audio files.",
	"license": "MIT",
	"repository": "lesjoursfr/audio-waveform",
	"homepage": "https://github.com/lesjoursfr/audio-waveform#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/audio-waveform/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.14.0"
	},
	"keywords": [
		"waveform",
		"generator",
		"audio"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"build": "npx tsc && cp -R src/audio-analyzer lib",
		"lint": "npx eslint --ext .ts --fix src/",
		"test": "ava"
	},
	"dependencies": {
		"electron": "^17.1.0",
		"mime-types": "^2.1.34",
		"uuid": "^8.3.2",
		"yargs": "^17.3.1"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/mime-types": "^2.1.1",
		"@types/node": "^16.11.26",
		"@types/uuid": "^8.3.4",
		"@types/yargs": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.13.0",
		"@typescript-eslint/parser": "^5.13.0",
		"ava": "^4.0.1",
		"eslint": "^8.10.0",
		"ts-node": "^10.5.0",
		"typescript": "^4.6.2"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"packageManager": "yarn@3.2.0"
}
