{
	"name": "@lesjoursfr/audio-waveform",
	"version": "2.0.0",
	"description": "A library to generate waveform for audio files.",
	"license": "MIT",
	"repository": "lesjoursfr/audio-waveform",
	"homepage": "https://github.com/lesjoursfr/audio-waveform#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/audio-waveform/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=16.15.1 || >=18.4.0"
	},
	"keywords": [
		"waveform",
		"generator",
		"audio"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write .",
		"build": "tsc && cp -R src/audio-analyzer lib",
		"test": "ava"
	},
	"dependencies": {
		"electron": "^20.1.1",
		"mime-types": "^2.1.35",
		"uuid": "^9.0.0",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.3",
		"@types/mime-types": "^2.1.1",
		"@types/node": "^16.11.56",
		"@types/uuid": "^8.3.4",
		"@types/yargs": "^17.0.12",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"ava": "^4.3.3",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.7.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.2"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"packageManager": "yarn@3.2.3"
}
