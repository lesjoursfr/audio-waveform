{
	"name": "@lesjoursfr/audio-waveform",
	"version": "3.0.0",
	"description": "A library to generate waveform for audio files.",
	"license": "MIT",
	"repository": "lesjoursfr/audio-waveform",
	"homepage": "https://github.com/lesjoursfr/audio-waveform#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/audio-waveform/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "18.x || 20.x"
	},
	"keywords": [
		"waveform",
		"generator",
		"audio"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write .",
		"build": "tsc && cp -R src/audio-analyzer lib",
		"test": "ava"
	},
	"dependencies": {
		"electron": "^25.1.0",
		"mime-types": "^2.1.35",
		"uuid": "^9.0.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@tsconfig/node18": "^2.0.1",
		"@types/mime-types": "^2.1.1",
		"@types/node": "^20.2.5",
		"@types/uuid": "^9.0.2",
		"@types/yargs": "^17.0.24",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"ava": "^5.3.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.3"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"packageManager": "yarn@3.6.0"
}
