{
	"name": "@lesjoursfr/audio-waveform",
	"version": "3.1.3",
	"description": "A library to generate waveform for audio files.",
	"license": "MIT",
	"repository": "lesjoursfr/audio-waveform",
	"homepage": "https://github.com/lesjoursfr/audio-waveform#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/audio-waveform/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "18.x || 20.x"
	},
	"keywords": [
		"waveform",
		"generator",
		"audio"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write .",
		"build": "tsc && cp -R src/audio-analyzer lib",
		"test": "mocha"
	},
	"dependencies": {
		"electron": "^28.1.0",
		"mime-types": "^2.1.35",
		"uuid": "^9.0.1",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.2",
		"@types/mime-types": "^2.1.4",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.10.6",
		"@types/uuid": "^9.0.7",
		"@types/yargs": "^17.0.32",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"mocha": "^10.2.0",
		"prettier": "^3.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"packageManager": "yarn@4.0.2"
}
